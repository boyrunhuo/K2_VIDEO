*{
    margin: 0;
    padding: 0;
}

body {
    min-width: 1200px;
    width: 100%;
    overflow: scroll;
}

/*播放页面头部*/
.header{
    border-bottom:1px solid grey;
    height:100px;
}
p#K{
    font-size: 80px;
    color: deepskyblue;
    position:absolute ;
    top: 5px;
    left: 20px;
}
p#TWO{
    font-size: 80px;
    color: red;
    position:absolute;
    top: 5px;
    left: 150px;
}
.fa-youtube-play{
    position: absolute;
    top: 25px;
    left: 80px;
}
.btn-default:hover{
    color: white;
    background:deepskyblue;
}
.header_action{
    position:relative;
    left:65%;
    top:45px;
}
#userPage{
    padding:8px 15px;
}
.btn{
    position:relative;
    margin-left:35px;
}

/*播放视频部分*/
.content-action{
    position:relative;
    top:20px;
    left:70px;
}
.videoName{
    font-size:20px;
}
.onlineVideo{
    position:relative;
    left:300px;
    top:30px;
    width:750px;
}
#downloadVideo{
    border:1px solid lightgrey;
    padding:8px 12px;
    border-radius:5px;
    margin-left:25px;
    color:#4F4F4F;
}
body a:hover{
    color: white;
    background-color:deepskyblue;
    text-decoration:none;
}