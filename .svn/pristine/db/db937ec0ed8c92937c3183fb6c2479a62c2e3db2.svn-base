*{
    margin: 0;
    padding: 0;
}

body {
    min-width: 1200px;
    width: 100%;
    overflow: scroll;
}


p#K{

   font-size: 80px;
    color: deepskyblue;
    position:absolute ;
    top: 5px;
    left: 20px;
}
p#TWO{
    font-size: 80px;
    color: red;
    position:absolute;
    top: 5px;
    left: 150px;
}

.fa-youtube-play{
 position: absolute;
    top: 25px;
    left: 80px;


}



.fa-sign-out{
    position: relative;
    top: 45px;
    left: 1150px;

    border-color: dodgerblue;

}

#navbtn{

    position: relative;
    left: 350px;
    top: 40px;
}

.btn-default:hover{
    color: white;
    background:deepskyblue;
}

.input-group {
    width: 350px;
    position: relative;
    left:650px;
    top: 8px;
}
.input-group select{
    position:relative;
    left:350px;
    top:-37px;
    height:35px;
}
.input-group input{
    height:35px;
    position:relative;
    top:-2px;

}


#head {
    border-bottom: solid 1px darkgray;
    height: 100px;
    background-color: white;
}
#exitBtn{
    position:relative;
    left:90%;
    top:-80px;

}

.btn-primary {
    background-color: white;
    color: black;
    margin-left: 15px;
    margin-top: 10px;
    height: 30px;
    font-size: 15px;

}
.input-group-addon{
   margin:0;
    padding:0;
    width:60px;
    height:35px;
    position:relative;
    left:350px;
    top:-56px;
}

.btn-primary:hover {
    background-color: deepskyblue;
    color: white;
}

ul li{
    list-style: none;
    float: left;
    margin-left: 60px;
    position: relative;
    left: 700px;
}
.videoname {
    position: absolute;
    left: 100px;
    z-index: 1;
}
.videooption {
    opacity: 0;
  position: relative;
}
a{
    text-decoration: none;
    color: black;
    z-index: 2;
}


/*视频元信息表格*/
.videoInfor{
    position:absolute;
    z-index:300;
    background-color:lightgrey;
    width:600px;
    height:400px;
    left:380px;
    top:140px;
    text-align:center;
}
#videoInforWord{
    position:relative;
    top:20px;
    font-size:30px;
    color:white;
    font-weight:500;
}
.getVideoInfor{
    position:relative;
    border:2px solid white;
    left:105px;
    top:30px;
}
.getVideoInfor th{
    text-align:center;
    width:200px;
    height:65px;
    border:2px solid white;
    color:#4F4F4F;
}
#videoInforConfirmBtn{
    position:relative;
    top:35px;
}

/*重命名*/
.user-renameVideo{
    position:absolute;
    left:35%;
    background-color:lightgrey;
    width:350px;
    height:160px;
    top:160px;
    text-align:center;
}
.user-renameVideo span{
    position:relative;
    top:25px;
    font-size:20px;
}
.user-renameVideo input{
    position:relative;
    top:30px;
    width:250px;
    height:30px;
}
#renameConfirmBtn{
    position:relative;
    top:30px;
    left:-20px;
}
#renameCancelBtn{
    position:relative;
    top:30px;
}

/*删除视频*/
.user-deleteVideo{
    position:absolute;
    left:35%;
    background-color:lightgrey;
    width:350px;
    height:160px;
    top:160px;
    text-align:center;
}
.user-deleteVideo span{
    position:relative;
    top:40px;
    font-size:20px;
}
#deleteConfirmBtn{
    position:relative;
    top:50px;
    left:-20px;
}
#deleteCancelBtn{
    position:relative;
    top:50px;
}

/*标签*/
.user-tagVideo{
    position:absolute;
    left:400px;
    background-color:lightgrey;
    width:500px;
    height:350px;
    top:160px;
    text-align:center;
}
.user-tagVideo span{
    position:relative;
    left:-122px;
    top:20px;
    font-size:20px;
}
.user-tagVideo textarea{
    position:relative;
    top:30px;

}
.user-tagVideo p{
    position:relative;
    top:30px;
    width:400px;
    left:38px;
    opacity: 0.7;
}
#tagConfirmBtn{
    position:relative;
    top:25px;
    left:-60px;
}
#tagCancelBtn{
    position:relative;
    top:25px;
    left:20px;
}

/*上传框*/
.upload_inform{
    position:absolute;
    width:250px;
    height:100px;
    top:130px;
    left:1150px;
}

