*{
    margin: 0;
    padding: 0;
}

body {
    min-width: 1200px;
    width: 100%;
    overflow: scroll;
    overflow-x:hidden;
}


p#K{
    font-size: 80px;
    color: deepskyblue;
    position:absolute ;
    top:-15px;
    left: 25px;
}
p#TWO{
    font-size: 80px;
    color: red;
    position:absolute;
    left: 150px;
    top:-15px;
}
.fa-youtube-play{
    position: absolute;
    top: 10px;
    left: 80px;
}

#head {
    border-bottom: solid 1px darkgray;
    height: 80px;
    background-color: white;
}

#navbtn{
    position: relative;
    left: 350px;
    top: 40px;
}

.btn-default:hover{
    color: white;
    background:deepskyblue;
}

.input-group {
    width: 350px;
    position: relative;
    left:650px;
    top: -25px;
}

.input-group input{
    height:35px;
    position:relative;
    top:-2px;
}
#userName{
    position:relative;
    left:950px;
    top:-35px;
    font-size:20px;
    color:#778899;
}
#exitBtn{
    position:relative;
    left:1230px;
    top:-65px;

}

#menu{
    height:65px;
}
.btn-primary {
    position:relative;
    left:100px;
    top:5px;
    background-color: white;
    color: black;
    margin-left: 15px;
    margin-top: 10px;
    height: 30px;
    font-size: 15px;
}
.input-group-addon{
    margin:0;
    padding:0;
    width:60px;
    height:35px;
    position:relative;
    left:350px;
    top:-37px;
}

.btn-primary:hover {
    background-color: deepskyblue;
    color: white;
}

#shareRoom-words{
    position:relative;
    left:70px;
    top:15px;
    font-size:23px;
    font-weight:500;
}

ul li{
    list-style: none;
    float: left;
    margin-left: 60px;
    position: relative;
    left: 700px;
}
.videoname {
    position: absolute;
    left: 100px;
    z-index: 1;
}
.videooption {
    opacity: 0;
    position: relative;
}
a{
    text-decoration: none;
    color: black;
    z-index: 2;
}
#main caption{
    position:relative;
    left:70px;
}

/*视频元信息表格*/
.videoInfor{
    position:absolute;
    z-index:300;
    background-color:#D3D3D3;
    width:650px;
    height:400px;
    left:380px;
    top:140px;
    text-align:center;
}
#videoInforWord{
    position:relative;
    top:20px;
    font-size:30px;
    color:white;
    font-weight:500;
}
.getVideoInfor{
    position:relative;
    border:2px solid white;
    left:25px;
    top:30px;;
}
.getVideoInfor th{
    text-align:center;
    width:150px;
    height:65px;
    border:2px solid white;
    color:#4F4F4F;
}
#videoInforConfirmBtn{
    position:relative;
    top:35px;
    left:-8px;
}

/*导航栏*/
.navbar{
    position:relative;
    top:27px;
    width:346px;
    left:250px;
}
.navbar ul li{
    padding:0;
    margin:0;
    position:relative;
    left:0;
}
.navbar-default .navbar-nav>.active>a, .navbar-default .navbar-nav>.active>a:focus, .navbar-default .navbar-nav>.active>a:hover {
    color: #555;
    background-color:#B0C4DE;
}