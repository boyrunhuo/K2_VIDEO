/*视频元信息表格*/
.videoInfor{
    position:absolute;
    z-index:300;
    background-color:#D3D3D3;
    width:650px;
    height:400px;
    left:350px;
    top:150px;
    text-align:center;
}
#videoInforWord{
    position:relative;
    top:20px;
    font-size:30px;
    color:white;
    font-weight:500;
}
.getVideoInfor{
    position:relative;
    border:2px solid white;
    left:25px;
    top:30px;
}
.getVideoInfor th{
    text-align:center;
    width:150px;
    height:65px;
    border:2px solid white;
    color:#4F4F4F;
}
#videoInforConfirmBtn{
    position:relative;
    top:40px;
    left:-25px;
}

/*重命名*/
.user-renameVideo{
    position:absolute;
    left:35%;
    background-color:#DCDCDC;
    width:350px;
    height:160px;
    top:200px;
    text-align:center;
}
.user-renameVideo span{
    position:relative;
    top:25px;
    font-size:20px;
}
.user-renameVideo input{
    position:relative;
    top:30px;
    width:250px;
    height:30px;
}
#renameConfirmBtn{
    position:relative;
    top:40px;
    left:-35px;
}
#renameCancelBtn{
    position:relative;
    top:40px;
    left:-25px;
}

/*删除视频*/
.user-deleteVideo{
    position:absolute;
    left:35%;
    background-color:lightgrey;
    width:350px;
    height:160px;
    top:200px;
    text-align:center;
}
.user-deleteVideo span{
    position:relative;
    top:40px;
    font-size:20px;
}
#deleteConfirmBtn{
    position:relative;
    top:70px;
    left:-40px;
}
#deleteCancelBtn{
    position:relative;
    top:70px;
    left:-25px;
}

/*标签*/
