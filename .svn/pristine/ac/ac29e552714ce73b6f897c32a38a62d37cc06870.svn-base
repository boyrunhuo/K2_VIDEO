* {
    margin: 0;
    padding: 0;

}

body {
    min-width: 1200px;
    width: 100%;
    overflow: scroll;
    /*background-color: cornflowerblue;*/
}
#header {
    background-color: white;
}

#header button {
    position: relative;
    left: 70%;
    top: 10px;
    z-index: 999;
}

